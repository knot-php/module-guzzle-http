{
  "name": "knot-php/module-guzzle-http",
  "description": "guzzlehttp/guzzle installer modules",
  "keywords": ["PHP","Guzzle","Module","Installer","kNot Framework"],
  "homepage": "https://github.com/knot-php/module-guzzle-http",
  "type": "library",
