    "knot-lib/module": "~0.7",
    "knot-lib/support": "~0.4",
    "knot-lib/http": "~0.3",
    "knot-php/module-knot-pipeline": "~0.2",
    "knot-php/module-stk2k-eventstream": "~0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "knotphp\\module\\guzzlehttp\\": "src/"
    }
  },
  "autoload-dev": {
